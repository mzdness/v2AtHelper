{
  "name": "Page Redder",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "content_scripts": [
    {
      "matches": ["https://www.v2ex.com/t/*"],
      "js": ["jquery.js", "addlink.js"]
    }
  ],
  "manifest_version": 2
}
